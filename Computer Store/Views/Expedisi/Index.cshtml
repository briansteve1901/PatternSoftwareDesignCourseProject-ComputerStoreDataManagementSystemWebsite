@model IEnumerable<Computer_Store.Models.Expedisi>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NamaExpedisi)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NamaKurir)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Telepon)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NamaExpedisi)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NamaKurir)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Telepon)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.IDExpedisi }) |
            @Html.ActionLink("Details", "Details", new { id=item.IDExpedisi }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.IDExpedisi })
        </td>
    </tr>
}

</table>
